{
    "name": "lampvlad/mailtrap-sdk",
    "description": "SDK for Maliltrap API",
    "keywords": [
        "lampvlad",
        "mailtrap-sdk"
    ],
    "homepage": "https://github.com/lampvlad/mailtrap-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vladislav Nalyvaiko",
            "email": "slavalampdev@googlemail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lampvlad\\MailtrapSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lampvlad\\MailtrapSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
